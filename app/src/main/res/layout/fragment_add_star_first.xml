<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#eee"
    android:clipToPadding="true"
    android:fitsSystemWindows="true"
    tools:context="cn.mycsoft.babygrowstar.frg.AddStarFirstActivityFragment"
    tools:showIn="@layout/activity_add_star_first">

    <GridView
        android:id="@android:id/list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#00eeeeee"
        android:numColumns="3"
        tools:listitem="@layout/task_item_grid_item" />

    <TextView
        android:id="@+id/empty"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:text="宝贝还没有任务模板" />

</FrameLayout>
